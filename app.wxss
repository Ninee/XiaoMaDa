@font-face {
    font-family: DIN;
    src: url(https://files.pay.dianjishenghuo.cn/mrys/fonts/din/DIN-Medium.otf);
}

page {
    background: #f7f7f7;
    z-index: -1;
    font-size: 24rpx;
    color: #2d2320;
    font-family: PingFangSC-Medium,PingFang SC;
}

view {
    box-sizing: border-box;
}

._img,image {
    max-width: 750rpx;
    width: 100%;
    height: 100%;
}

.icon_img {
    width: 48rpx;
    height: 48rpx;
}

.noMore {
    width: 100%;
    color: #bebebe;
    text-align: center;
    margin: 10rpx 0;
}

.ly-flex-center,.ly-flex-center-center {
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}

.ly-flex-center-center {
    -webkit-box-pack: center;
    justify-content: center;
}

.ly-flex-end {
    -webkit-box-align: end;
    align-items: flex-end;
}

.ly-flex-end,.ly-flex-start {
    display: flex;
}

.ly-flex-start {
    -webkit-box-align: start;
    align-items: flex-start;
}

.ly-flex-stretch {
    display: flex;
    -webkit-box-align: stretch;
    align-items: stretch;
}

.ly-flex-baseline,.ly-flex-baseline-between {
    display: flex;
    -webkit-box-align: baseline;
    align-items: baseline;
}

.ly-flex-baseline-between {
    -webkit-box-pack: justify;
    justify-content: space-between;
}

.ly-flex-start-around {
    -webkit-box-align: start;
    align-items: flex-start;
}

.ly-flex-center-around,.ly-flex-start-around {
    display: flex;
    justify-content: space-around;
}

.ly-flex-center-around {
    -webkit-box-align: center;
    align-items: center;
}

.ly-flex-start-between {
    -webkit-box-align: start;
    align-items: flex-start;
}

.ly-flex-center-between,.ly-flex-start-between {
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
}

.ly-flex-center-between {
    -webkit-box-align: center;
    align-items: center;
}

.ly-flex-center-warp {
    -webkit-box-align: center;
    align-items: center;
}

.ly-flex-between-warp,.ly-flex-center-warp {
    display: flex;
    flex-wrap: wrap;
}

.ly-flex-between-warp {
    -webkit-box-pack: justify;
    justify-content: space-between;
}

.ly_flex_center_column,.ly_flex_column {
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
}

.ly_flex_center_column {
    -webkit-box-align: center;
    align-items: center;
}

.ly_flex_between_column {
    -webkit-box-pack: justify;
    justify-content: space-between;
}

.ly_flex_between_column,.ly_flex_center_around_column {
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
}

.ly_flex_center_around_column {
    -webkit-box-align: center;
    align-items: center;
    justify-content: space-around;
}

.flex,.flex_be {
    display: flex;
    flex-wrap: wrap;
}

.flex_be {
    -webkit-box-pack: justify;
    justify-content: space-between;
}

.flex_arr {
    display: flex;
    justify-content: space-around;
    flex-wrap: wrap;
}

page::after {
    position: fixed;
    content: "";
    left: -1000px;
    top: -1000px;
    animation: shadow-preload .1s;
    animation-delay: 3s;
}

@-webkit-keyframes shadow-preload {
    0% {
        background-image: url(https://cdn.dcloud.net.cn/img/shadow-grey.png);
    }

    100% {
        background-image: url(https://cdn.dcloud.net.cn/img/shadow-grey.png);
    }
}

@keyframes shadow-preload {
    0% {
        background-image: url(https://cdn.dcloud.net.cn/img/shadow-grey.png);
    }

    100% {
        background-image: url(https://cdn.dcloud.net.cn/img/shadow-grey.png);
    }
}

[bind-data-custom-hidden="true"],[data-custom-hidden="true"] {
    display: none!important;
}