.topNav {
    width: 100vh;
    top: 0;
}

.top,.topNav {
    position: fixed;
    left: 0;
    z-index: 100;
    background: #fff;
}

.top {
    width: 100vw;
    height: 88rpx;
    line-height: 88rpx;
    display: flex;
    flex-wrap: wrap;
    -webkit-box-align: center;
    align-items: center;
}

.kite-classify-scroll {
    width: 100%;
    height: 160rpx;
    overflow: hidden;
    white-space: nowrap;
    background: #fff;
    font-size: 24rpx;
    box-shadow: 0 -2rpx 0 0 #f7f7f7;
    position: fixed;
    left: 0;
    z-index: 10;
    border-bottom: 2rpx solid #f7f7f7;
}

.kite-classify-scroll .kite-classify-cell {
    display: inline-block;
    width: 130rpx;
    height: 160rpx;
}

.kite-classify-scroll .kite-classify-cell .sel_kite-classify-cell-item {
    padding-bottom: 30rpx;
}

.kite-classify-scroll .kite-classify-cell .kite-classify-cell-item {
    height: 160rpx;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: end;
    justify-content: flex-end;
    padding-bottom: 20rpx;
}

.kite-classify-scroll .kite-classify-cell .kite-classify-cell-item .type_imgs {
    width: 70rpx;
    height: 70rpx;
    border-radius: 16rpx;
    background: #ffd08c;
}

.kite-classify-scroll .kite-classify-cell .kite-classify-cell-item .type_imgs image {
    width: 60rpx;
    height: 60rpx;
    border-radius: 16rpx;
}

.kite-classify-scroll .kite-classify-cell .kite-classify-cell-item .select_imgs {
    width: 86rpx;
    height: 86rpx;
}

.kite-classify-scroll .kite-classify-cell .kite-classify-cell-item .select_imgs image {
    width: 76rpx;
    height: 76rpx;
    border-radius: 16rpx;
}

.kite-classify-scroll .kite-classify-cell .kite-classify-cell-item .type_title {
    color: #000;
    text-align: center;
    margin-top: 12rpx;
    min-width: 70rpx;
    padding: 0 10rpx;
    height: 30rpx;
    line-height: 30rpx;
}

.kite-classify-scroll .kite-classify-cell .kite-classify-cell-item .select_title {
    background: #09d334;
    border-radius: 16rpx;
    color: #fff;
    text-align: center;
    margin-top: 12rpx;
    min-width: 70rpx;
    padding: 0 10rpx;
    height: 30rpx;
}

.notice_box {
    padding: 0 20rpx;
    border-bottom: 2rpx solid #f7f7f7;
}

.notice_box .white_boxs {
    height: 40rpx;
    line-height: 40rpx;
    border-radius: 16rpx;
    background-color: #fff;
    display: flex;
}

.notice_box .white_boxs image {
    width: 40rpx;
    height: 24rpx;
    margin: 0 10rpx 0 22rpx;
}

.notice_box .white_boxs .notice {
    height: 40rpx;
    background-color: #fff;
    -webkit-box-flex: 1;
    flex: 1;
    font-size: 20rpx;
    color: #333;
    border-radius: 0 16rpx 16rpx 0;
}

.recover_type,.recover_type .left_list {
    display: flex;
}

.recover_type .left_list {
    width: 170rpx;
    font-size: 28rpx;
    color: #323232;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    background: #f7f7f7;
}

.recover_type .left_list .left_list_item {
    height: 90rpx;
    text-align: center;
    border-bottom: 0rpx solid #999;
    font-weight: 700;
}

.recover_type .left_list .left_list_item .type_imgs {
    width: 6rpx;
    height: 50rpx;
    background: #09d334;
    border-radius: 4rpx;
    margin: 0 30rpx 0 6rpx;
}

.recover_type .left_list .left_list_item .not_type_imgs {
    width: 6rpx;
    height: 50rpx;
    background: #f7f7f7;
    border-radius: 4rpx;
    margin: 0 30rpx 0 6rpx;
}

.recover_type .left_list .sel_color {
    background-color: #fff;
}

.recover_type .right_list {
    -webkit-box-flex: 1;
    flex: 1;
    height: 100%;
    background-color: #fff;
}

.recover_type .right_list .list_con .list_img {
    width: 100%;
    height: 200rpx;
    padding: 20rpx;
}

.recover_type .right_list .list_con .list_img image {
    width: 100%;
    height: 100%;
}

.recover_type .right_list .list_con .list_item {
    height: 140rpx;
    display: flex;
    color: #333;
    margin: 0 0 30rpx 0rpx;
    padding: 0 40rpx 0 20rpx;
}

.recover_type .right_list .list_con .list_item .list_item_title {
    font-size: 28rpx;
}

.recover_type .right_list .list_con .list_item .list_item_discrib {
    font-size: 20rpx;
    color: #999;
}

.recover_type .right_list .list_con .list_item .list_item_price {
    font-size: 20rpx;
    color: #ff5762;
}

.recover_type .right_list .list_con .list_item .play_tips text {
    font-size: 20rpx;
    color: #999;
    width: 70rpx;
    height: 24rpx;
    border-radius: 4rpx;
    margin-right: 10rpx;
    padding: 0;
}

.recover_type .right_list .list_con .list_item .btns {
    height: 40rpx;
    width: 100%;
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    align-items: center;
}

.recover_type .right_list .list_con .list_item .btns .play_order {
    width: 40rpx;
    height: 40rpx;
    background: #09d334;
    border-radius: 20rpx;
    color: #fff;
    font-size: 30rpx;
    padding-bottom: 4rpx;
}

.min_length {
    display: -webkit-box;
    overflow: hidden;
    white-space: normal!important;
    text-overflow: ellipsis;
    word-wrap: break-word;
    -webkit-line-clamp: 1;
}

.min_length,.phone_btn {
    -webkit-box-orient: vertical;
}

.phone_btn {
    background: none;
    border: 0rpx solid #fff;
    padding: 0;
    line-height: 1;
    font-size: 24rpx;
    display: flex;
    -webkit-box-flex: 1;
    flex: 1;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-align: start;
    align-items: flex-start;
    width: 40rpx;
}

.phone_btn:after {
    border: none;
}

.play_order_btn_box {
    width: 100%;
    position: fixed;
    left: 0;
    bottom: 0rpx;
    padding: 0rpx 20rpx 50rpx 0rpx;
    background: #fff;
}

.play_order_btn_box .play_order_top {
    height: 102rpx;
    color: #333;
    font-size: 36rpx;
    position: relative;
    padding: 0 20rpx;
    margin-left: 20rpx;
}

.play_order_btn_box .play_order_top .play_order_top_title {
    width: 100%;
    height: 102rpx;
    line-height: 102rpx;
    position: absolute;
    left: 0;
    z-index: -1;
    text-align: center;
}

.play_order_btn_box .play_order_top image {
    width: 30rpx;
    height: 30rpx;
}

.play_order_btn_box .recovery_goods_items {
    height: 110rpx;
    border-top: 2rpx solid #f5f5f5;
}

.play_order_btn_box .recovery_goods_items,.play_order_btn_box .recovery_goods_items .recovery_goods_items_l {
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    align-items: center;
}

.play_order_btn_box .recovery_goods_items .recovery_goods_items_l {
    color: #333;
    font-size: 24rpx;
}

.play_order_btn_box .recovery_goods_items .recovery_goods_items_l image {
    width: 70rpx;
    height: 70rpx;
    margin-right: 26rpx;
}

.play_order_btn_box .recovery_goods_items .recovery_goods_items_l .weight_num {
    font-size: 20rpx;
    color: #666;
}

.play_order_btn_box .recovery_goods_items .recovery_goods_items_r {
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    align-items: center;
    font-size: 24rpx;
    color: #f3390e;
}

.play_order_btn_box .recovery_goods_items .recovery_goods_items_r image {
    width: 30rpx;
    height: 30rpx;
    margin-left: 20rpx;
}

.play_order_btn_box .play_order_btn {
    height: 90rpx;
    background: #fff;
    box-shadow: 0rpx 0rpx 10rpx 0rpx rgba(0,0,0,.1);
    border-radius: 46rpx;
    display: flex;
    position: relative;
}

.play_order_btn_box .play_order_btn .play_order_l {
    -webkit-box-flex: 1;
    flex: 1;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    border-radius: 46rpx 0 0 46rpx;
}

.play_order_btn_box .play_order_btn .play_order_l .play_order_img {
    width: 80rpx;
    height: 80rpx;
    background: url(data:image/png;base64,);
    background-size: 80rpx 80rpx;
    margin: 0 20rpx 0 30rpx;
}

.play_order_btn_box .play_order_btn .play_order_l .play_order_img .play_order_type_num {
    font-size: 16rpx;
    height: 24rpx;
    line-height: 24rpx;
    min-width: 24rpx;
    background-color: red;
    margin: 8rpx 0 0 50rpx;
    display: inline-block;
    color: #fff;
    border-radius: 50%;
    text-align: center;
}

.play_order_btn_box .play_order_btn .play_order_l .play_order_content {
    color: #f3390e;
    font-size: 28rpx;
}

.play_order_btn_box .play_order_btn .play_order_l .play_order_content .play_order_tips {
    font-size: 20rpx;
    color: #666;
}

.play_order_btn_box .play_order_btn .play_order_r {
    width: 200rpx;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    color: #fff;
    font-size: 28rpx;
    background-color: #09d334;
    border-radius: 0 46rpx 46rpx 0;
}

.play_order_btn_box .play_order_btn .over_reduce_tips_box {
    -webkit-box-align: end;
    align-items: flex-end;
    position: absolute;
    top: -40rpx;
    right: 0rpx;
    width: 70%;
    height: 40rpx;
}

.play_order_btn_box .play_order_btn .over_reduce_tips_box .sanjiao {
    width: 0;
    height: 0;
    border: 8rpx solid transparent;
    border-top-color: #fb2e3e;
}

.play_order_btn_box .play_order_btn .over_reduce_tips_box .over_reduce_tips {
    padding: 0 48rpx;
    height: 30rpx;
    line-height: 30rpx;
    text-align: center;
    background: -webkit-gradient(linear,left top,left bottom,from(#ff7364),to(#fb2e3e));
    background: -webkit-linear-gradient(top,#ff7364,#fb2e3e);
    background: linear-gradient(180deg,#ff7364,#fb2e3e);
    border-radius: 15rpx;
    font-size: 20rpx;
    color: #fff;
    font-weight: lighter;
}

.sel_weight_popup_box {
    width: 580rpx;
    background: #fff;
    box-shadow: 0 2rpx 0 0 #f5f5f5,0 -2rpx 0 0 #f5f5f5;
    border-radius: 16rpx;
    padding-top: 24rpx;
}

.sel_weight_popup_box .titles1 {
    font-size: 36rpx;
    color: #333;
    text-align: center;
    font-weight: 700;
}

.sel_weight_popup_box .titles2 {
    font-size: 24rpx;
    color: #666;
    margin-top: 20rpx;
    text-align: center;
}

.sel_weight_popup_box .weight_btns {
    flex-wrap: wrap;
    padding: 30rpx 70rpx 0rpx;
}

.sel_weight_popup_box .weight_btns .goods_items {
    margin: 0 30rpx 40rpx;
    width: 160rpx;
    height: 60rpx;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    color: #999;
    border: 2rpx solid #ccc;
    background: #fff;
    border-radius: 30rpx;
}

.sel_weight_popup_box .weight_btns .sel_goods_items {
    color: #fff;
    border: none;
    background-color: #09d334;
}

.sel_weight_popup_box .confirm_cancle {
    display: flex;
    height: 78rpx;
    border-top: 2rpx solid #e6e6e6;
}

.sel_weight_popup_box .confirm_cancle view {
    -webkit-box-flex: 1;
    flex: 1;
    text-align: center;
    height: 78rpx;
    line-height: 78rpx;
    font-size: 28rpx;
    color: #333;
    font-weight: 700;
}