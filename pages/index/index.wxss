.hot_swiper_box {
    width: 100%;
    height: 250rpx;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
}

.hot_swiper_box .swiper {
    height: 250rpx;
}

.hot_swiper_box .swiper,.hot_swiper_box .swiper swiper-item {
    width: 100%;
    border-radius: 20rpx;
    overflow: hidden;
}

.hot_swiper_box .swiper swiper-item {
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    height: 100%;
}

.hot_swiper_box .swiper swiper-item image {
    width: 100%;
    height: 100%;
    border-radius: 20rpx;
    animation: hiddens .5s;
    animation-fill-mode: forwards;
}

.hot_swiper_box .swiper swiper-item image.le-active {
    animation: centers .5s;
    animation-fill-mode: forwards;
}

@-webkit-keyframes centers {
    0% {
        transform: scale(.75);
        width: 240rpx;
        height: 240rpx;
    }

    100% {
        transform: scale(1);
        width: 350rpx;
        height: 250rpx;
    }
}

@keyframes centers {
    0% {
        transform: scale(.75);
        width: 240rpx;
        height: 240rpx;
    }

    100% {
        transform: scale(1);
        width: 350rpx;
        height: 250rpx;
    }
}

@-webkit-keyframes hiddens {
    0% {
        transform: scale(1);
        width: 100%;
        height: 250rpx;
    }

    100% {
        transform: scale(.75);
        width: 240rpx;
        height: 240rpx;
    }
}

@keyframes hiddens {
    0% {
        transform: scale(1);
        width: 100%;
        height: 250rpx;
    }

    100% {
        transform: scale(.75);
        width: 240rpx;
        height: 240rpx;
    }
}

