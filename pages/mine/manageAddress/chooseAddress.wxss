.out_padding {
    padding: 22rpx 20rpx 20rpx;
}

.voice {
    height: 100rpx;
    background: #fff;
    border-radius: 16rpx;
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    align-items: center;
    padding: 0 0rpx 0 20rpx;
    color: #666;
    font-size: 24rpx;
}

.voice .voice_title {
    width: 500rpx;
    height: 100rpx;
    line-height: 100rpx;
}

.voice .voice_r {
    padding-right: 20rpx;
}

.voice .voice_r,.voice .voice_r .voice_img {
    display: flex;
}

.voice .voice_r .voice_img {
    width: 76rpx;
    height: 100rpx;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
}

.voice .voice_r .voice_img image {
    width: 30rpx;
    height: 30rpx;
}

.voice_input {
    background: #fff;
    border-radius: 16rpx;
    padding: 20rpx;
}

.voice_input .voice_input_text {
    width: 100%;
    height: 80rpx;
    color: #333;
    text-align: justify;
}

.voice_input .voice_input_bottom {
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: justify;
    justify-content: space-between;
    color: #fff;
    font-size: 24rpx;
    margin-top: 20rpx;
}

.voice_input .voice_input_bottom .not_distinguish {
    width: 120rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    background: #999;
    border-radius: 20rpx;
}

.voice_input .voice_input_bottom .distinguish {
    background-color: #09d334;
}

.voice_input .voice_input_bottom .voice_input_bottom_r,.voice_input .voice_input_bottom .voice_input_bottom_r .voice_btn {
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}

.voice_input .voice_input_bottom .voice_input_bottom_r .voice_btn {
    color: #666;
    font-size: 20rpx;
    margin-right: 18rpx;
}

.voice_input .voice_input_bottom .voice_input_bottom_r .voice_btn image {
    width: 30rpx;
    height: 30rpx;
    margin-right: 6rpx;
}

.white_bg {
    background-color: #fff;
    padding: 0 20rpx 20rpx;
    margin-top: 20rpx;
    border-radius: 16rpx;
}

.white_bg .btn {
    color: #fff;
    font-size: 36rpx;
    text-align: center;
    height: 80rpx;
    line-height: 80rpx;
    background: #09d334;
    border-radius: 20rpx;
    margin-top: 16rpx;
}

.line_height {
    height: 88rpx;
    line-height: 88rpx;
    font-size: 26rpx;
    color: #42424d;
    border-bottom: 2rpx solid #f5f5f5;
}

.delete_form,.line_height {
    display: flex;
}

.delete_form {
    color: #999;
    font-size: 24rpx;
    -webkit-box-align: center;
    align-items: center;
}

.delete_form image {
    width: 30rpx;
    height: 30rpx;
    margin-right: 6rpx;
}

.delete_form view {
    padding-bottom: 2rpx;
}

.little_color {
    color: #838399;
    font-weight: 300;
}

.old_address {
    background-color: #fff;
    border-radius: 16rpx;
    overflow: hidden;
}

.old_address .address {
    width: 100%;
    -webkit-box-align: center;
    align-items: center;
    border-top: 2rpx solid #e6e6e6;
}

.old_address .address,.old_address .address .address_l {
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
}

.old_address .address .address_l {
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    font-size: 30rpx;
    padding: 18rpx 0 18rpx 30rpx;
    position: relative;
}

.old_address .address .address_l .sel_add {
    width: 0;
    height: 0;
    border-top: 30rpx solid #09d334;
    border-right: 30rpx solid transparent;
    position: absolute;
    top: 0;
    left: 0;
}

.voice_input_popup .voice_input_popup_box {
    width: 500rpx;
    height: 420rpx;
    background-color: #fff;
    border-radius: 16rpx;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-align: center;
    align-items: center;
    color: #333;
    font-size: 36rpx;
    padding: 32rpx 0 40rpx;
}

.voice_input_popup .voice_input_popup_box .voice_popup_imgs {
    width: 150rpx;
    height: 150rpx;
    margin-top: 20rpx;
    position: relative;
}

.voice_input_popup .voice_input_popup_box .voice_popup_imgs .voice_popup_imgs_a {
    width: 70rpx;
    height: 90rpx;
    position: absolute;
    top: 30rpx;
    left: 40rpx;
}

.voice_input_popup .voice_input_popup_box .voice_popup_imgs .voice_popup_imgs_b {
    width: 150rpx;
    height: 150rpx;
}

.voice_input_popup .voice_input_popup_box .voice_run {
    animation: voicerun 1.5s infinite;
    animation-timing-function: linear;
}

@-webkit-keyframes voicerun {
    0% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(1turn);
    }
}

@keyframes voicerun {
    0% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(1turn);
    }
}

.voice_input_popup .voice_input_popup_box .voice_popup_tips {
    font-size: 24rpx;
    margin-top: 22rpx;
    text-align: center;
}

.voice_input_popup .voice_input_popup_box .voice_over {
    width: 300rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    background: #f3390e;
    border-radius: 30rpx;
    color: #fff;
    font-size: 30rpx;
    margin-top: 20rpx;
}

.often_address {
    height: 68rpx;
    line-height: 68rpx;
    padding-left: 30rpx;
    color: #333;
    font-weight: 700;
}

.min_length {
    display: -webkit-box;
    overflow: hidden;
    white-space: normal!important;
    text-overflow: ellipsis;
    word-wrap: break-word;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}