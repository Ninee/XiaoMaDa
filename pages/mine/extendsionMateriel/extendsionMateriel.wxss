page {
    background-color: #f3f3f3;
    font-size: 28rpx;
    color: #333;
    letter-spacing: 0;
    word-wrap: break-word;
}

button,form,image,input,label,page,textarea,view {
    box-sizing: border-box;
}

button:after {
    border: none;
}

image {
    display: block;
    height: auto;
    width: auto;
}

input,textarea {
    font-size: 28rpx;
}

.flex_col {
    -webkit-box-orient: horizontal;
    flex-direction: row;
    -webkit-box-align: center;
    align-items: center;
    align-content: center;
}

.flex_col,.flex_row {
    display: flex;
    -webkit-box-direction: normal;
    flex-wrap: nowrap;
    -webkit-box-pack: start;
    justify-content: flex-start;
}

.flex_row {
    -webkit-box-orient: vertical;
    flex-direction: column;
    -webkit-box-align: start;
    align-items: flex-start;
    align-content: flex-start;
}

.flex_col .flex_grow {
    width: 0;
}

.flex_col .flex_grow,.flex_row .flex_grow {
    -webkit-box-flex: 1;
    flex-grow: 1;
}

.flex_col.flex_wrap {
    flex-wrap: wrap;
}

.flex_col.flex_center,.flex_row.flex_center {
    -webkit-box-pack: center;
    justify-content: center;
}

.flex_col.flex_space {
    -webkit-box-pack: justify;
    justify-content: space-between;
}

.flex_col.flex_col_2>view {
    width: 50%;
}

.flex_col.flex_col_3>view {
    width: 33.33333%;
}

.flex_col.flex_col_4>view {
    width: 25%;
}

.flex_col.flex_col_5>view {
    width: 20%;
}

.flex_col.flex_col_6>view {
    width: 16.66666%;
}

