}

.box {
    width: 100%;
    height: 100vh;
    background-color: #fff;
    overflow: auto;
    position: relative;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
    background-image: url(data:image/png;base64,);
    background-size: 100% 50%;
    background-position: bottom;
    background-repeat: no-repeat;
}

.box .rules {
    width: 120rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    color: #d70000;
    font-size: 32rpx;
    background: #f9d949;
    opacity: .8;
    border-radius: 30rpx 0 0 30rpx;
    position: absolute;
    top: 46rpx;
    right: 0;
    z-index: 100;
    font-weight: 700;
}

.box .btns {
    width: 100%;
    margin-top: 50rpx;
}

.box .btns .shar_btns {
    width: 300rpx;
    height: 80rpx;
    line-height: 80rpx;
    background: #fff;
    border-radius: 40rpx;
    color: #333;
    font-size: 32rpx;
}

.box .btns .shar_btns image {
    width: 50rpx;
    height: 50rpx;
    margin-right: 10rpx;
}

.swipers {
    width: 100%;
    height: 1020rpx;
}

.swipers .qrcode_style {
    width: 100%;
    height: 180rpx;
    overflow: hidden;
    position: absolute;
    bottom: 254rpx;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    padding-right: 26rpx;
    animation: noqrcodeStyle .5s infinite;
    animation-fill-mode: forwards;
    animation-iteration-count: 1;
}

.swipers .qrcode_style.sel_img_bg {
    animation: qrcodeStyle .5s infinite;
    animation-fill-mode: forwards;
    animation-iteration-count: 1;
}

@-webkit-keyframes qrcodeStyle {
    0% {
        bottom: 254rpx;
    }

    100% {
        bottom: 234rpx;
    }
}

@keyframes qrcodeStyle {
    0% {
        bottom: 254rpx;
    }

    100% {
        bottom: 234rpx;
    }
}

@-webkit-keyframes noqrcodeStyle {
    0% {
        bottom: 234rpx;
    }

    100% {
        bottom: 254rpx;
    }
}

@keyframes noqrcodeStyle {
    0% {
        bottom: 234rpx;
    }

    100% {
        bottom: 254rpx;
    }
}

.swipers .qrcode_style image {
    width: 180rpx;
    height: 180rpx;
    transform: scale(.9);
    animation: noImgStyle .5s infinite;
    animation-fill-mode: forwards;
    animation-iteration-count: 1;
}

.swipers .qrcode_style image.le-active {
    transform: scale(1);
    animation: qecodes .5s infinite;
    animation-fill-mode: forwards;
    animation-iteration-count: 1;
}

@-webkit-keyframes qecodes {
    0% {
        transform: scale(.9);
    }

    100% {
        transform: scale(1);
    }
}

@keyframes qecodes {
    0% {
        transform: scale(.9);
    }

    100% {
        transform: scale(1);
    }
}

@-webkit-keyframes noImgStyle {
    0% {
        transform: scale(1);
    }

    100% {
        transform: scale(.9);
    }
}

@keyframes noImgStyle {
    0% {
        transform: scale(1);
    }

    100% {
        transform: scale(.9);
    }
}

.le-img {
    width: 100%;
    height: 100%;
    display: block;
    transform: scale(.9);
    transition: transform .3s ease-in-out 0s;
    transition: transform .3s ease-in-out 0s,-webkit-transform .3s ease-in-out 0s;
    border-radius: 4px;
}

.le-img.le-active {
    transform: scale(1);
    box-shadow: 0 4px 10px 0 rgba(0,0,0,.3);
}

