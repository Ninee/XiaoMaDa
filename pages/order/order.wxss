}

.phone_btn {
    background: none;
    line-height: 1;
    font-size: 24rpx;
    -webkit-box-flex: 1;
    flex: 1;
    -webkit-box-orient: vertical;
    flex-direction: column;
    -webkit-box-pack: justify;
