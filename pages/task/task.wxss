.topNav {
    width: 100vh;
    position: fixed;
    top: 0;
}

.top,.topNav {
    color: #fff;
    left: 0;
    z-index: 100;
}

.top {
    width: 100vw;
    height: 88rpx;
    line-height: 88rpx;
    display: flex;
    flex-wrap: wrap;
    -webkit-box-align: center;
    align-items: center;
    position: absolute;
}

.top_content {
    width: 100%;
    height: 540rpx;
    position: relative;
}

.top_content .contents {
    width: 100%;
    height: 142rpx;
    padding: 0 136rpx;
    position: absolute;
    left: 0;
    top: 324rpx;
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
}

.top_content .contents .data_border {
    width: 232rpx;
    height: 141rpx;
    position: relative;
}

.top_content .contents .data_border .datas {
    width: 100%;
    padding: 34rpx 0;
    position: absolute;
    left: 0;
    top: 0;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    align-items: center;
    font-size: 36rpx;
    color: red;
    font-weight: 500;
}

.top_content .contents .data_border .datas .titles {
    font-size: 24rpx;
    color: #333;
}

.notice_bar {
    display: flex;
    width: 100%;
    height: 50rpx;
    line-height: 50rpx;
    padding-left: 80rpx;
    background: hsla(0,0%,88.2%,.2);
    position: absolute;
    left: 0;
    bottom: 0;
}

.notice_bar .horn {
    width: 20rpx;
    height: 20rpx;
    margin-right: 16rpx;
}

.notice_bar .swiper {
    width: 100%;
    height: 50rpx;
    line-height: 50rpx;
}

.notice_bar .swiper .swiper_items {
    display: flex;
}

.notice_bar .swiper .swiper_items .swiper_itemsL {
    -webkit-box-flex: 1;
    flex: 1;
    color: #fff;
}

.notice_bar .swiper .swiper_items .swiper_itemsR {
    -webkit-box-flex: 1;
    flex: 1;
    color: #fff72a;
}

.min_length {
    display: -webkit-box;
    overflow: hidden;
    white-space: normal!important;
    text-overflow: ellipsis;
    word-wrap: break-word;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

.tasks_list {
    background-color: #b31a14;
    padding: 20rpx;
}

.tasks_list .lists {
    height: 120rpx;
    background: #fff;
    border-radius: 10rpx;
    margin-bottom: 20rpx;
    padding: 20rpx 40rpx;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}

.tasks_list .lists .imgs {
    margin-right: 20rpx;
}

.tasks_list .lists .imgs image {
    width: 80rpx;
    height: 80rpx;
}

.tasks_list .lists .lists_r {
    width: 100%;
    height: 80rpx;
    -webkit-box-align: center;
    align-items: center;
}

.tasks_list .lists .lists_r,.tasks_list .task_contents {
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
}

.tasks_list .task_contents {
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
}

.tasks_list .task_contents .task_dis {
    font-size: 28rpx;
    color: #333;
}

.tasks_list .task_contents .task_title {
    height: 80rpx;
    line-height: 80rpx;
}

.tasks_list .task_contents .discribe {
    font-size: 20rpx;
    height: 20rpx;
    color: #999;
    margin: 6rpx 0;
}

.tasks_list .task_contents .discribe image {
    width: 100%;
    height: 100%;
    margin: 0 10rpx;
}

.tasks_list .task_contents .discribe .prize {
    color: red;
}

.tasks_list .task_contents .progress_out {
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    font-size: 16rpx;
    color: red;
}

.tasks_list .task_contents .progress_out .progress {
    width: 180rpx;
    height: 10rpx;
    border-radius: 10rpx;
    background: -webkit-gradient(linear,left top,right top,color-stop(80%,red),color-stop(0,#e6e6e6));
    background: -webkit-linear-gradient(left,red 80%,#e6e6e6 0);
    background: linear-gradient(90deg,red 80%,#e6e6e6 0);
    margin-right: 12rpx;
}

.tasks_list .to_over {
    color: #fff;
    background: -webkit-gradient(linear,left top,right top,from(#f25145),to(#eb3549));
    background: -webkit-linear-gradient(left,#f25145,#eb3549);
    background: linear-gradient(90deg,#f25145,#eb3549);
}

.tasks_list .to_over,.tasks_list .to_over1 {
    width: 145rpx;
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
    border-radius: 25rpx;
}

.tasks_list .to_over1 {
    background-color: #e5e5e5;
    color: #999;
}

.tasks_list .has_over {
    background: -webkit-gradient(linear,left top,right top,from(#f6b44d),to(#ff7a60));
    background: -webkit-linear-gradient(left,#f6b44d,#ff7a60);
    background: linear-gradient(90deg,#f6b44d,#ff7a60);
}

.pops {
    width: 580rpx;
    height: 760rpx;
    background-image: url(data:image/png;base64,);
    background-size: 100% 100%;
    background-repeat: no-repeat;
    padding-top: 158rpx;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-align: center;
    align-items: center;
}

.pops .pops_title {
    font-size: 36rpx;
    color: #fdf4e5;
    font-weight: 700;
}

.pops .pops_img {
    width: 260rpx;
    height: 260rpx;
    margin-top: 34rpx;
}

.pops .pops_dis {
    font-size: 48rpx;
    font-weight: 700;
    color: #fff;
}

.pops .pops_btn {
    width: 450rpx;
    height: 80rpx;
    line-height: 80rpx;
    background: -webkit-gradient(linear,left bottom,left top,from(#eead6d),to(#f9daa7));
    background: -webkit-linear-gradient(bottom,#eead6d,#f9daa7);
    background: linear-gradient(0deg,#eead6d,#f9daa7);
    border-radius: 40rpx;
    text-align: center;
    margin-top: 46rpx;
    font-size: 36rpx;
    font-weight: 700;
    color: #d70800;
    text-shadow: 0 2px 0 rgba(254,240,136,.63);
}

.pops .konw_detail {
    font-size: 24rpx;
    color: #fff;
    width: 100%;
    height: 70rpx;
    line-height: 70rpx;
    text-align: center;
}