.upDoor {
    width: 200rpx;
    height: 70rpx;
    line-height: 70rpx;
    background-color: #ffc933;
    border-radius: 35rpx;
    position: fixed;
    right: 0;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
}

.upDoor_img {
    width: 60rpx;
    height: 60rpx;
    margin-left: 6rpx;
    margin-top: 6rpx;
}

.upDoor_img image {
    width: 100%;
    height: 100%;
    border-radius: 20rpx;
}

.font {
    font-size: 30rpx;
    color: #333;
    margin-left: 20rpx;
}